<%=render "users/list",book: @book,user: @user %>
	<div class="col-lg-9">
	<h1>Users</h1>
	<table class="table">
		<thead>
		<tr>
			<th>image</th>
			<th>name</th>
			<th></th>
		</tr>
	</thead>
	<tbody>
		<% @users.each do |user| %>
		<tr>
			<td>
				<%= attachment_image_tag user,:profile_image, :fill,20,20,class: " pull-left profile-thumb",fallback: "no_image.jpg", :width => "20", :height => "20" %>
			</td>
			<td>
				<span><%= user.name %></span>
			</td>
			<td>
				<%=link_to "Show",user_path(user.id)%>
			</td>
		</tr>
	</tbody>
		<% end %>
	</table>
	</div>
</div>
</div>